@article{Hauser2014,
abstract = {This paper presents a method for generating dynamically feasible, keyframe-interpolating motions for robots undergoing contact, such as in legged locomotion and manipulation. The first stage generates a twice-differentiable interpolating path that obeys kinematic contact constraints up to a user-specified tolerance. The second stage optimizes speeds along the path to minimize time while satisfying dynamic constraints. The method supports velocity, acceleration, and torque constraints, and polyhedral contact friction constraints at an arbitrary number of contact points. The method is numerically stable, and empirical running time is weakly linear in the number of degrees of freedom and polynomial in the time-domain grid resolution. Experiments demonstrate that full-body motions for robots with 100 degrees of freedom and dozens of contact points are calculated in seconds.},
author = {Hauser, Kris},
journal = {The International Journal of Robotics Research},
keywords = {algorithms,legged locomotion,manipulation,trajectory optimization},
number = {9},
pages = {1231--1250},
title = {{Fast interpolation and time-optimization with contact}},
volume = {33},
year = {2014}
}
@article{Hauser2008,
abstract = {In this paper we study the quasi-static motion of large{\$}\backslash{\$}nlegged robots that have many degrees of freedom. While{\$}\backslash{\$}ngaited walking may suffice on easy ground, rough and steep{\$}\backslash{\$}nterrain requires unique sequences of footsteps and postural{\$}\backslash{\$}nadjustments specifically adapted to the terrain's local{\$}\backslash{\$}ngeometric and physical properties. In this paper we present{\$}\backslash{\$}na planner that computes these motions by combining graph{\$}\backslash{\$}nsearching to generate a sequence of candidate footfalls{\$}\backslash{\$}nwith probabilistic sample-based planning to generate{\$}\backslash{\$}ncontinuous motions that reach these footfalls. To improve{\$}\backslash{\$}nmotion quality, the probabilistic planner derives its{\$}\backslash{\$}nsampling strategy from a small set of motion primitives{\$}\backslash{\$}nthat have been generated offline. The viability of this{\$}\backslash{\$}napproach is demonstrated in simulation for the six-legged{\$}\backslash{\$}nLunar vehicle ATHLETE and the humanoid HRP-2 on several{\$}\backslash{\$}nexample terrains, including one that requires both hand and{\$}\backslash{\$}nfoot contacts and another that requires rappelling.},
author = {Hauser, Kris and Bretl, Timothy and Latombe, Jean-Claude and Harada, K and Wilcox, B},
journal = {The International Journal of Robotics Research},
number = {11-12},
pages = {1325--1349},
