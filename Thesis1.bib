@article{Hill1988,
annote = { This paper describes an algorithm for matching two-dimensional images that does not depend on the extraction of features, a step which is olten difficult or impractical. We believe that our algorithm is as good or better than previously published algorithms in most situations, and dis- tinctly advantageous when feature extraction is a serious impediment. It combines computational efficiency with robustness against noisy data. It has been tested with two kinds of data.
1.The algorithm matches a two-dimensional image, in the form of a set of points, to a line segment model. It estimates a positive congruence (i.e., a translation plus a rotation) that moves the image onto the model. The algorithm is most appropriate for matching when the required congruence is small, and it seems very well suited to motion-tracking. However, good convergence has also been achieved for large congruences. The algorithm is robust against noise, and can deal with images that have both missing and extraneous points. Asymptotically, the computational complexity of the algorithm is O(NM) where N is the number of image points and M is the number of model segments. This compares favorably with other matching techniques.
Experimental results have been obtained for two kinds of images:
(1) points obtained from an optical range finder, and (2) edge points extracted from an intensity image. Only the former results are included, for lack of space.},
author = {Hill, Murray},
journal = {Optimization},
pages = {252--258},
title = {{On the Congruence of Noisy Images to Line Segment Models}},
year = {1988}
}
