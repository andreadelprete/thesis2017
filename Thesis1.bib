keywords = { Ocular, Ocular: physiology,Animals,Attention,Cues,Depth Perception,Depth Perception: physiology,Environment,Fixation,Macaca nemestrina,Motion Perception,Motion Perception: physiology,Neurons,Neurons: physiology,Photic Stimulation,Retina,Retina: physiology,Robotics,Somatosensory Cortex,Somatosensory Cortex: cytology,Somatosensory Cortex: physiology,Visual Pathways,Visual Pathways: physiology},
month = {jul},
