isbn = {978-1-4244-6674-0},
issn = {21530858},
journal = {2010 IEEE/RSJ International Conference on Intelligent Robots and Systems},
keywords = {Control Architectures and Programming,Motion Control,Path Planning for Manipulators},
pages = {5927--5933},
title = {{Online replanning for reactive robot motion: Practical aspects}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=5649645},
year = {2010}
