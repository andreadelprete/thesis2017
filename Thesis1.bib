@article{Gautier1997,
abstract = {This paper presents a new approach to identify the minimum dynamic{\$}\backslash{\$}nparameters of robots using least squares techniques (LS) and a power{\$}\backslash{\$}nmodel. Theoretical analysis is carried out from a filtering point of{\$}\backslash{\$}nview and clearly shows the superiority of the power model over the{\$}\backslash{\$}nenergy one and over the dynamic identification model which has been used{\$}\backslash{\$}nto carry out a classical ordinary LS estimation and a new weighted LS{\$}\backslash{\$}nestimation. These results are checked from comparing experimental{\$}\backslash{\$}nidentification of the dynamic parameters of a planar SCARA prototype{\$}\backslash{\$}nrobot},
