@article{Abba1999,
abstract = {In this paper, we present a method for robots modeling called ldquobidirectional{\$}\backslash{\$}ndynamic modelingrdquo. This new method takes into account the gear{\$}\backslash{\$}nefficiency and the direction of power transmission in the gears.{\$}\backslash{\$}nEpicyclic gearboxes have often different efficiencies in the two{\$}\backslash{\$}ndirections of power transmission. The characteristics of the chain{\$}\backslash{\$}nof transmission must then be taken into consideration in order to{\$}\backslash{\$}ndescribe the dynamic behavior of robots. The two directions of power{\$}\backslash{\$}nflow can indeed occur in robot motions. Depending on that direction{\$}\backslash{\$}nthe dynamic model is different. The bidirectional dynamic modeling{\$}\backslash{\$}nis experimentally applied to a bipedal walking robot. Our method{\$}\backslash{\$}nexhibits a better accuracy over classical modeling. Moreover, when{\$}\backslash{\$}napplied to computed torque control, the bidirectional model increases{\$}\backslash{\$}nthe tracking performances.},
author = {Abba, Gabriel and Chaillet, Nicolas},
